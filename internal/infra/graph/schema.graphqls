type Order {
    id: String!
    Price: Float!
    Tax: Float!
    FinalPrice: Float!
}

input OrderInput {
    id : String!
    Price: Float!
    Tax: Float!
}

input ListOrderInput {
    offset: String!
    limit: String!
}

type Query {
    listOrders(input:ListOrderInput): [Order!]!
}

type Mutation {
    createOrder(input: OrderInput): Order
}